{
    "action_manifest_version": 0,
    "alias_info": {},
    "app_key": "openvr.component.vrcompositor",
    "bindings": {
        "/actions/lasermouse": {
            "poses": [
                {
                    "output": "/actions/lasermouse/in/Pointer",
                    "path": "/user/head/pose/raw"
                }
            ],
            "sources": [
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/lasermouse/in/leftclick"
                        }
                    },
                    "mode": "button",
                    "path": "/user/gamepad/input/a"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/lasermouse/in/leftclick"
                        }
                    },
                    "mode": "button",
                    "parameters": {
                        "click_activate_threshold": "0.5",
                        "click_deactivate_threshold": "0.45"
                    },
                    "path": "/user/gamepad/input/trigger_right"
                }
            ]
        },
        "/actions/system": {
            "sources": [
                {
                    "inputs": {
                        "single": {
                            "output": "/actions/system/in/opendashboard"
                        }
                    },
                    "mode": "complex_button",
                    "path": "/user/gamepad/input/start"
                }
            ]
        }
    },
    "category": "steamvr_input",
    "controller_type": "micmap_controller",
    "description": "",
    "name": "Default VR Dashboard bindings for MicMap Virtual Controller",
    "options": {},
